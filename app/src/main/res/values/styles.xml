<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark1</item>
        <item name="colorAccent">@color/colorPrimaryDark1</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
    </style>

    <style name="AppTheme.PopMe">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="WhiteCheck">
        <item name="colorAccent">#74ECDA</item>
    </style>

    <style name="selectedDate">
        <item name="colorAccent">#74ECDA</item>
    </style>

    <style name="EditAbout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">#ffffff</item>
        <item name="boxStrokeWidth">2dp</item>
        <item name="colorAccent">#ffffff</item>
    </style>

    <style name="AddEventInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="colorAccent">@color/text_Color</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>

